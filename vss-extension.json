{
    "manifestVersion": 1.0,
    "id": "dfin-task-templates",
    "version": "0.0.72",
    "name": "Donnelley Financial Solutions Task Templates Extension",
    "description": "TODO Fill this out",
    "public": false,
    "publisher": "TimCardwell",
    "icons": {
        "default": "static/images/dfin_logo.jpg"
    },
